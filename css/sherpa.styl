/*-----Menu navegacion-----*/
#main-menu
	margin-top 30px
	float right
#main-menu a
	color #65646a
#main-menu a:hover
	color #f6303e
#main-menu.nav-pills>li.active>a,
#main-menu.nav-pills>li.active>a:hover,
#main-menu.nav-pills>li.active>a:focus,
#main-menu.nav-pills>li>a:hover
	border-top solid 3px #f6303e
	margin-top -3px
	background none
	border-radius 0px
#main-menu.nav-pills>li.active>a,
#main-menu.nav-pills>li.active>a:hover,
#main-menu.nav-pills>li.active>a:focus
	color #f6303e
#mobile-menu-button
	float right
	width 44px
	margin 20px 10px
#mobile-main-menu
	padding 0
	display none
#mobile-main-menu a
	display block
	width 100%
	padding 10px 0
	border-top 1px solid #eee
	color #65646a
	text-align center
	text-transform uppercase
#mobile-main-menu li
	margin 0
#mobile-main-menu a:hover
	color #fff
	text-decoration none
	background-color #f6303e
#footer
