		float right
		margin 0 3px
/*-----index-----*/
.items
	padding 0
.imagenes
	width 100%

a:hover span.text-content {
  opacity: 1;
}
.text-content {
  background: rgba(246,48,62,0.5);
  color: white;
  cursor: pointer;
  display: block;
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
  opacity: 0;
  -webkit-transition: opacity 500ms;
  -moz-transition: opacity 500ms;
  -o-transition: opacity 500ms;
  transition: opacity 500ms;
}
.text-content span {
  display: block;
  text-align: center;
  border 1px solid #fff
  width 50%
  margin auto
  padding 10px 10px
  position: absolute;
  top: 50%;
  left: 25%;
}
